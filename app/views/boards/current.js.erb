$("#column-planned, #column-started, #column-done").remove();
$("#planned-panel-heading").after("<%= j(render 'planned_cards') %>");
$("#started-panel-heading").after("<%= j(render 'started_cards') %>");
$("#done-panel-heading").after("<%= j(render 'done_cards') %>");

$("#column-planned, #column-started, #column-done").addClass("actionable"); //Restores sortable functionality
